<section class="container-fluid">
  <div class="col-sm-4 col-sm-offset-4">
    <% if !@report.nil? %>
      <%= form_for @report, url: student_report_path(@report.student,@report.date) do |f| %>
        <%= render "form_categories", f: f %>
      <% end %>

  <% if !@report.nil? %>
    <%= form_for @report, url: student_report_path(@report.student,@report.date) do |f| %>
    <div class="col-sm-4 col-sm-offset-4">
      <%= render "form_categories", f: f %>
    </div>
    <% end %>
  </div>
</section>
