<section class="container-fluid">
  <div class="col-sm-4 col-sm-offset-4">
    <% if !@report.nil? %>
      <%= form_for @report, url: student_report_path(@report.student,@report.date) do |f| %>
        <%= render "form_categories", f: f, report_notes: @report.report_notes %>
      <% end %>
    <% end %>
  </div>
</section>
